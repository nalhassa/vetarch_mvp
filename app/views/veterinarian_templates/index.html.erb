<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All veterinarians
    </h1>

    <a href="/veterinarians/new" class="btn btn-block btn-outline-secondary">
      Add a new veterinarian
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          First name
        </th>

        <th>
          Last name
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @veterinarians.each do |veterinarian| %>
      <tr>
        <!-- Display veterinarian.id start -->
        <td>
          <%= veterinarian.id %>
        </td>
        <!-- Display veterinarian.id end -->

        <!-- Display veterinarian.first_name start -->
        <td>
          <%= veterinarian.first_name %>
        </td>
        <!-- Display veterinarian.first_name end -->

        <!-- Display veterinarian.last_name start -->
        <td>
          <%= veterinarian.last_name %>
        </td>
        <!-- Display veterinarian.last_name end -->

        <!-- Display veterinarian.created_at start -->
        <td>
          <%= time_ago_in_words(veterinarian.created_at) %> ago
        </td>
        <!-- Display veterinarian.created_at end -->

        <!-- Display veterinarian.updated_at start -->
        <td>
          <%= time_ago_in_words(veterinarian.updated_at) %> ago
        </td>
        <!-- Display veterinarian.updated_at end -->

        <td>
          <a href="/veterinarians/<%= veterinarian.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
